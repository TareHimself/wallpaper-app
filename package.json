    "prettier": "2.8.4",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "~4.5.4",
    "@types/node": "^18.14.2"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "axios": "^0.26.1",
    "history": "^5.3.0",
    "macaddress": "^0.5.2",
    "react": "^18.2.0",
